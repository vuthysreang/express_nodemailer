{
  "name": "nodemailer-nodeexpress",
  "version": "1.0.0",
  "description": "nodemailer-nodeexpress project using docker with sequelize",
  "main": "app.js",
  "scripts": {
    "start": "sequelize-cli db:migrate && sequelize-cli db:seed:all && node ./bin/www",
    "dev": "sequelize-cli db:migrate && sequelize-cli db:seed:all && nodemon ./bin/www",
    "debug": "DEBUG=slecret:* npm run dev"
  },
  "keywords": [
    "NodeJS",
    "ExpressJS",
    "Sequelize",
    "postgres",
    "docker",
    "git",
    "vscode"
  ],
  "author": "Vuthy Sreang",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.5",
    "cors": "^2.8.5",
    "debug": "~4.3.2",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.6",
    "email-validator": "^2.0.4",
    "express": "~4.17.1",
    "http-errors": "~1.8.0",
    "jade": "~1.11.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.10.0",
    "nodemailer": "^6.6.2",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.6.5"
  },
  "devDependencies": {
    "nodemon": "^2.0.12",
    "sequelize-cli": "^6.2.0"
  }
}
